use gtk;

fn onquit(arg1, arg2) {
    gtk.main_quit();
}

fn main() {
    window = gtk.Window();
    window.connect("delete-event", onquit);
    button = gtk.Button("Hello");
    button.connect("clicked", fn(arg1) { print("test"); });
    window.add(button);

    window.show_all();

    gtk.main();
}

main();

