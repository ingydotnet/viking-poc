use gtk;

func onquit(arg1, arg2) {
    gtk.main_quit();
}

func main() {
    window = gtk.Window();
    window.connect("delete-event", onquit);
    button = gtk.Button("Hello");
    button.connect("clicked", func(arg1) { print("test"); });
    window.add(button);

    window.show_all();

    gtk.main();
}

main();

